/**
 * Demultiplexor.
 * {x,y} = {in,0} if sel == 0
 *         {0,in} if sel == 1
 */

CHIP DMux {
    IN in, sel;
    OUT x, y;

    PARTS:
    Not(x = sel, out = notsel);
    And(x = in, y = notsel, out = x);
    And(x = in, y = sel, out = y);

}
