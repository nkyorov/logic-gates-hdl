/**
 * Or gate:
 * result = 1 if (x==1 or y==1),
 *          0 otherwise
 */

CHIP Or {
    IN x, y;
    OUT out;

    PARTS:
    Nand(x=x, y=x, out=temp1);
    Nand(x=y, y=y, out=temp2);
    Nand(x=temp1, y=temp2, out=out);

}
