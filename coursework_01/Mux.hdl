/**
 * Multiplexor. If sel==1 then out = y else out = x.
 */

CHIP Mux {
    IN x, y, sel;
    OUT out;

    PARTS:
    Not(x = sel, out = notsel);
    And(x = x, y = notsel, out = temp1);
    And(x = sel, y = y, out = temp2);
    Or(x = temp1, y = temp2, out = out);

}
